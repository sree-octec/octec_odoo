<odoo>
  <data>
    <record id="custom_models_view_tree" model="ir.ui.view">
      <field name="name">custom.models.view.tree</field>
      <field name="model">custom.models</field>
      <field name="arch" type="xml">
        <list>
          <field name="name"/>
        </list>
      </field>
    </record>
    <record id="custom_models_view_form" model="ir.ui.view">
      <field name="name">custom.models.view.form</field>
      <field name="model">custom.models</field>
      <field name="arch" type="xml">
        <form string="Personal Details">
            <sheet>
              <group>
                <field name="name"/>
              </group>
            </sheet>
        </form>
      </field>
    </record>
    <record id="action_custom_models" model="ir.actions.act_window">
      <field name="name">Custom Model</field>
      <field name="res_model">custom.models</field>
      <field name="view_mode">list,form</field>
    </record>
    
  </data>
</odoo>