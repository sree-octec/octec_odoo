<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="practice_task_patient_view_form" model="ir.ui.view">
        <field name="name">practice.task.patient.view.form </field>
        <field name="model">practice.task.patient</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group string="Partner Info">
                        <field name="name"/>
                        <field name="phone"/>
                        <field name="email"/>
                        <field name="street"/>
                        <field name="city"/>
                        <field name="zip"/>
                        <field name="country_id"/>
                    </group>

                    <group string="Patient Info">
                        <field name="total_appointments"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="practice_task_patient_view_tree" model="ir.ui.view">
        <field name="name">practice.task.patient.view.tree</field>
        <field name="model">practice.task.patient</field>
        <field name="arch" type="xml">
            <list>
                <field name="total_appointments"/>
            </list>
        </field>
    </record>
    <record id="practice_task_patient_action" model="ir.actions.act_window">
        <field name="name">Prcatice Customers</field>
        <field name="res_model">practice.task.patient</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>