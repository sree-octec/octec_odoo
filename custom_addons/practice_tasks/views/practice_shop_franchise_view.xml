<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="practice_shop_franchise_view_form" model="ir.ui.view">
        <field name="name">practice.shop.franchise.view.form </field>
        <field name="model">practice.shop.franchise</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group string="Shop Franchise Info">
                        <field name="name"/>
                        <field name="email"/>
                        <field name="phone_number"/>
                        <field name="branch"/>
                    </group>
                    <group>
                        <field name="expiry_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="practice_shop_franchise_view_tree" model="ir.ui.view">
        <field name="name">practice.shop.franchise.view.tree</field>
        <field name="model">practice.shop.franchise</field>
        <field name="arch" type="xml">
            <list>
                <field name="phone_number"/>
                <field name="branch"/>
                <field name="expiry_date"/>
            </list>
        </field>
    </record>
    <record id="practice_shop_franchise_action" model="ir.actions.act_window">
        <field name="name">Prcatice Shop Franchise</field>
        <field name="res_model">practice.shop.franchise</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>